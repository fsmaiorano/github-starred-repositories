{
    "presets": [
        "react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "test": "./test",
                    "underscore": "lodash"
                }
            }
        ],
        [
            "transform-class-properties"
        ]
    ]
}